<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<Operation Class="PatchOperationReplace">
		<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="Alien_Moyo"]/alienRace/generalSettings/maleGenderProbability</xpath>
		<value>
			<maleGenderProbability>0.5</maleGenderProbability>
		</value>
	</Operation>
	
	
	<!-- Names -->
	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="Alien_Moyo"]/race</xpath>
		<value>
			<nameGenerator>NamerMoyoMale</nameGenerator>
			<nameGeneratorFemale>NamerMoyoFemale</nameGeneratorFemale>
		</value>
	</Operation>
	
</Patch>